/* pages/integral/integral.wxss */

page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f4f4f4;
}

.tabs {
  width: 100%;
}

.integral-record {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 36rpx;
}

.record-item {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  align-items: center;
}

.record-text {
  width: 678rpx;
  font-size: 26rpx;
  color: #000;
  text-align: left;
  margin-top: 20rpx;
}

.record-time {
  width: 678rpx;
  font-size: 26rpx;
  color: #6f6f6f;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  text-align: left;
}

.view-line {
  width: 100%;
  height: 2rpx;
  background-color: #f4f4f4;
}

.info_null {
  text-align: center;
  align-content: center;
  color: #6f6f6f;
  font-size: 30rpx;
  margin-top: 100rpx;
}

.integral-list {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 36rpx;
}

.listItem {
  width: 678rpx;
  height: 260rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  margin-bottom: 20rpx;
  justify-content: space-between;
  background-image: url(data:image/png;base64,);
}

.item-left {
  display: flex;
  flex-direction: column;
  margin-left: 72rpx;
}

.item-price {
  font-size: 72rpx;
  color: #fff;
}

.item-text {
  font-size: 48rpx;
  color: #fff;
}

.item-logo {
  width: 143rpx;
  height: 156rpx;
  margin-right: 86rpx;
}
